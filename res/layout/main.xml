<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/widget32"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="#f9f9f9"
	android:orientation="vertical">
	<TextView android:id="@+id/word" android:layout_width="fill_parent"
		android:layout_height="60px" android:text="@string/hello" android:textColor="#333333"
		android:textSize="22px" android:gravity="center_horizontal">
	</TextView>
	<Button android:id="@+id/e" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:textSize="25px" android:textColor="#333333"
		android:text="e" android:layout_weight="1">
	</Button>
	<Button android:id="@+id/en" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:textSize="25px" android:textColor="#333333"
		android:text="en" android:layout_weight="1">
	</Button>
	<Button android:id="@+id/em" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:textSize="25px" android:textColor="#333333"
		android:text="em" android:layout_weight="1">
	</Button>
	<Button android:id="@+id/es" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:textSize="25px" android:textColor="#333333"
		android:text="es" android:layout_weight="1">
	</Button>
	<Button android:id="@+id/er" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:textSize="25px" android:textColor="#333333"
		android:text="er" android:layout_weight="1">
	</Button>
	<TextView android:id="@+id/output" android:layout_width="fill_parent"
		android:layout_height="25px" android:text="@string/hello"
		android:textSize="15px" android:textStyle="bold" android:textColor="#cc0000"
		android:gravity="center_horizontal">
	</TextView>
	<TextView android:id="@+id/totalResult" android:layout_width="fill_parent"
		android:layout_height="25px" android:text="@string/hello" android:textColor="#333333"
		android:textSize="15px" android:textStyle="bold" android:gravity="center_horizontal">
	</TextView>
	<TextView android:id="@+id/consecutiveResult"
		android:layout_width="fill_parent" android:layout_height="25px"
		android:text="@string/hello" android:textSize="15px" android:textColor="#333333"
		android:textStyle="bold" android:gravity="center_horizontal">
	</TextView>
	<TextView android:id="@+id/record" android:layout_width="fill_parent"
		android:layout_height="25px" android:text="@string/hello" android:textColor="#333333"
		android:textSize="15px" android:textStyle="bold" android:gravity="center_horizontal">
	</TextView>
</LinearLayout>